  KEY `fk_sb_line_item_fulfillment_shop_id_idx` (`shop_id`),
  KEY `fk_sb_line_item_fulfillment_order_id_idx` (`order_id`),
  KEY `sb_line_item_fulfillment_shop_id_order_id` (`shop_id`,`order_id`),
  CONSTRAINT `fk_sb_line_item_fulfillment_fulfillment_id` FOREIGN KEY (`fulfillment_id`) REFERENCES `sb_fulfillments` (`id`) ON DELETE CASCADE ON UPDATE NO ACTION,
  CONSTRAINT `fk_sb_line_item_fulfillment_order_id` FOREIGN KEY (`order_id`) REFERENCES `sb_order` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_sb_line_item_fulfillment_shop_id` FOREIGN KEY (`shop_id`) REFERENCES `shops` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
