  PRIMARY KEY (`id`),
  KEY `relation_id` (`relation_id`),
  KEY `price_rule_id` (`price_rule_id`),
  CONSTRAINT `sb_price_rule_customer_ibfk_2` FOREIGN KEY (`relation_id`) REFERENCES `sb_customer` (`id`) ON DELETE CASCADE,
  CONSTRAINT `sb_price_rule_customer_ibfk_4` FOREIGN KEY (`price_rule_id`) REFERENCES `sb_price_rule` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=38 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

