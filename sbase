  `payload` longtext COLLATE utf8mb4_unicode_ci NOT NULL,
  `response` longtext COLLATE utf8mb4_unicode_ci NOT NULL,
  `webhook_request_id` varchar(32) COLLATE utf8mb4_unicode_ci NOT NULL,
