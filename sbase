  KEY `third_app_id` (`third_app_id`),
  KEY `shop_id_topic_status` (`shop_id`,`topic`,`status`),
  KEY `address` (`address`),
  KEY `status` (`status`),
  KEY `shop_status` (`shop_id`,`status`),
  KEY `created_at` (`created_at`),
  CONSTRAINT `sb_webhook_logs_ibfk_1` FOREIGN KEY (`shop_id`) REFERENCES `shops` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `sb_webhook_logs_ibfk_2` FOREIGN KEY (`third_app_id`) REFERENCES `third_app` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
