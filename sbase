  `created_at` datetime NOT NULL,
  `updated_at` datetime NOT NULL,
  `metadata` text COLLATE utf8_unicode_ci,
  `product_type` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `has_custom_options` tinyint(1) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `variant_id` (`variant_id`),
  KEY `order_id` (`order_id`),
  CONSTRAINT `sb_line_item_ibfk_2` FOREIGN KEY (`order_id`) REFERENCES `sb_order` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
