CREATE TABLE `sb_review_product_group` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `shop_id` int(11) NOT NULL,
  `name` varchar(255) DEFAULT NULL,
  `type` tinyint(2) DEFAULT NULL COMMENT '1 - collection, 2 - tag, 3 - vendor''',
